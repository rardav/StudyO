{
  "Routes": [
    // Catalog
    {
      "DownstreamPathTemplate": "/api/Courses",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "catalog.api",
          "Port": "80"
        }
      ],
      "UpstreamPathTemplate": "/Courses",
      "UpstreamHttpMethod": [
        "GET",
        "POST"
      ],
      "SwaggerKey": "Courses"
    },
    {
      "DownstreamPathTemplate": "/api/Courses/{id}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "catalog.api",
          "Port": "80"
        }
      ],
      "UpstreamPathTemplate": "/Courses/{id}",
      "UpstreamHttpMethod": [
        "GET",
        "PUT",
        "DELETE"
      ]
    },
    // Reviews
    {
      "DownstreamPathTemplate": "/api/Reviews",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "reviews.api",
          "Port": "80"
        }
      ],
      "UpstreamPathTemplate": "/Reviews",
      "UpstreamHttpMethod": [
        "POST"
      ],
      "SwaggerKey": "Reviews"
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000"
  },
  "SwaggerEndPoints": [
    {
      "Key": "Reviews",
      "Config": [
        {
          "Name": "Reviews API",
          "Version": "v1",
          "Url": "http://localhost:5011/swagger/v1/swagger.json"
        }
      ]
    }
  ]
}